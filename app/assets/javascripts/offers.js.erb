var type;
$(document).ready(function() {
  $('#type-select').on('change', function(e) {
    type = $('#type-select').val();
    if(type == "service") {
      $('#nature-select-service').removeClass("hidden");
      $('#nature-select-rent').addClass("hidden");
      $('#nature-select-sell').addClass("hidden");
    }
    if(type == "sell") {
      $('#nature-select-sell').removeClass("hidden");
      $('#nature-select-rent').addClass("hidden");
      $('#nature-select-service').addClass("hidden");
    }
    if(type =="rent") {
      $('#nature-select-rent').removeClass("hidden");
      $('#nature-select-service').addClass("hidden");
      $('#nature-select-sell').addClass("hidden");
    }
  });
});